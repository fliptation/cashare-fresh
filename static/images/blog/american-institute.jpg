        // Mobile tap toggle  ONLY ONE OPEN AT A TIME
        $toggles.on('touchstart', function(e) {
            lastTouch = Date.now();
            e.stopPropagation();

            var $this = $(this);

            if ($this.hasClass('hovered')) {
                $this.removeClass('hovered'); // close if same toggle
            } else {
                $toggles.removeClass('hovered'); // close all others
                $this.addClass('hovered'); // open this one
            }
        });

        // Block ghost clicks after touch
        $toggles.on('click', function(e) {
            if (Date.now() - lastTouch < 500) {
                e.preventDefault();
                return false;
            }
        });

        // Tap outside to close all
        $(document).on('touchstart', function() {
            $toggles.removeClass('hovered');
        });
    }
